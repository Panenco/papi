    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "ts-node": "^10.8.0",
    "tsc-alias": "^1.6.7",
    "typedoc": "^0.22.16",
