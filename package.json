{
  "name": "@panenco/papi",
  "version": "1.6.0",
  "description": "Panenco papi",
  "author": "Panenco <info@panenco.com>",
  "repository": "git@github.com:Panenco/papi.git",
  "license": "MIT",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "private": false,
  "scripts": {
    "build": "tsc && tsc-alias",
    "lint": "eslint",
    "build:clean": "pnpm i && pnpm run clean:build && pnpm run build",
    "clean:build": "rm -rf ./build",
    "generate-docs": "pnpm exec typedoc",
    "watchDeletedFiles": "tsx scripts/deletedFileWatcher.script.ts",
    "watch": "tsc -w",
    "test": "NODE_PATH=./build mocha -r source-map-support/register build/tests/**/*.test.js --exit",
    "coverage": "nyc pnpm run test"
  },
  "peerDependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.18.2"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.0",
    "openapi3-ts": "^4.1.2",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.4",
    "routing-controllers-openapi": "^4.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/express": "^4.17.17",
    "@types/glob": "^8.1.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "chai": "^4.3.7",
    "chokidar": "^3.5.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "typedoc": "^0.24.7",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^4.7.2"
  },
  "pnpm": {
    "overrides": {
      "string-width": "^4.2.2"
    }
  },
  "packageManager": "pnpm@8.3.1"
}
